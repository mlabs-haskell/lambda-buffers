module Foo

import Foo.Bar
import Plutus.V1 (Address, Value, Datum, PlutusData)
import Prelude (Eq)

prod A = (FooSum Address Value Datum)

derive Eq A
derive PlutusData A

prod B = (FooProd Address Value Datum)

derive Eq B
derive PlutusData B

prod C = (FooRec Address Value Datum)

derive Eq C
derive PlutusData C

prod D = (FooComplicated Address Value Datum)

derive Eq D
derive PlutusData D
