  },
  "devDependencies": {
    "buffer": "6.0.3",
    "esbuild": "0.18.11",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "esbuild-plugin-wasm": "^1.1.0",
    "node-polyfill-webpack-plugin": "2.0.1",
    "webpack": "5.88.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "prettier": {
    "arrowParens": "avoid"
  }
