{
  "name": "lbr-prelude",
  "type": "module",
  "version": "1.0.0",
  "description": "LambdaBuffers Prelude runtime",
  "main": "app/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "spago test",
    "bundle": "spago bundle-module -m Test.Main --to dist/output.js",
